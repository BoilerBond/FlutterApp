{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "LOCAL EMULATOR",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define",
                "USE_FIREBASE_EMULATOR=true",
            ]
        },
        {
            "name": "FlutterApp",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define",
                "USE_FIREBASE_EMULATOR=false",
            ]
        },
        {
            "name": "FlutterApp (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": [
                "--dart-define",
                "USE_FIREBASE_EMULATOR=false",
            ]
        },
        {
            "name": "FlutterApp (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define",
                "USE_FIREBASE_EMULATOR=false",
            ]
        }
    ]
}